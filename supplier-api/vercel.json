{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/supplier/(.*)",
      "methods": ["OPTIONS"],
      "dest": "api/index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://supplier.wedealize.com",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, PATCH, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://supplier.wedealize.com",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
